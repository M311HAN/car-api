{
    "version": 2,
    "name": "car-api",
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      },
      {
        "src": "backend/car_server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/car_server.js"
      },
      {
        "source": "/(.*)",
        "destination": "/public/index.html"
      }
    ]
  }
  
  
  
  
  
  